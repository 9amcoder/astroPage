  <title>Steve Sultan - Official Site </title>
  <meta name="title" content="Steve Sultan - Software Developer">
  <meta name="description" content="Diligent software engineer with 5+ years experience. To provide innovative and cutting-edge business solutions for the  clients.">
  <meta name="copyright" content="Steve Sultan">

    <!-- Open Graph / Facebook -->
    <meta property="og:type" content="website">
    <meta property="og:url" content="https://www.stevesultan.com/">
    <meta property="og:title" content="Steve Sultan - Software Developer">
    <meta property="og:description" content="Diligent software engineer with 5+ years experience. To provide innovative and cutting-edge business solutions for the  clients.">
    <meta property="og:image" content="https://i.postimg.cc/pdT0fxr4/metatag-img.jpg">

    <!-- Twitter -->
    <meta property="twitter:card" content="summary_large_image">
    <meta property="twitter:url" content="https://www.stevesultan.com/">
